@using LendWeb.ViewModels.Users

@model UserPageModel

@{
    ViewData["Title"] = Model.User.UserName+"'s profile";
}

<h2>@String.Format("{0}'s profile", Model.User.UserName)</h2>

<div class="col-md-12">
    <div class="col-md-6">
        <h3>Info</h3>
        <table class="table">
            <tr>
                <td>User</td>
                <td>@Model.User.UserName</td>
            </tr>
            <tr>
                <td>Joined</td>
                <td>@Html.DisplayFor(x => x.User.Joined)</td>
            </tr>
        </table>
    </div>
</div>

<hr />
